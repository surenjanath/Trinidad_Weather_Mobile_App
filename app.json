{
  "expo": {
    "name": "TT Weather Watch",
    "slug": "tt-weather-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ttweather",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Official weather forecast for Trinidad and Tobago - Simple, clean, and focused weather information from TTMS",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ttweather.watch"
    },
    "android": {
      "package": "com.ttweather.watch",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1E3A8A"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
